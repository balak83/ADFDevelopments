{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DFBK1"
		},
		"LS_Destination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Destination'"
		},
		"LS_Import_CSV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Import_CSV'"
		},
		"LS_Sourrce_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Sourrce'"
		},
		"LS_SqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SqlServer'"
		},
		"ls_dest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dest'"
		},
		"blob_trigger_properties_pl_blob_trrigger_parameters_file_name": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"blob_trigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/85ad60f7-d1fb-4876-a32f-22223044717e/resourceGroups/RG_FreeTrial/providers/Microsoft.Storage/storageAccounts/source1blob1"
		},
		"tumbling_trigger_properties_pipeline_parameters_start_time": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowStartTime"
		},
		"tumbling_trigger_properties_pipeline_parameters_end_time": {
			"type": "string",
			"defaultValue": "@trigger().outputs.windowEndTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DFBK1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Destination_connectionString": {
						"value": "[parameters('LS_Destination_connectionString')]"
					},
					"LS_Import_CSV_connectionString": {
						"value": "[parameters('LS_Import_CSV_connectionString')]"
					},
					"LS_Sourrce_connectionString": {
						"value": "[parameters('LS_Sourrce_connectionString')]"
					},
					"LS_SqlServer_connectionString": {
						"value": "[parameters('LS_SqlServer_connectionString')]"
					},
					"ls_dest_connectionString": {
						"value": "[parameters('ls_dest_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DFBK1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DFBK1_ArmTemplate_0')]"
			]
		},
		{
			"name": "DFBK1_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DFBK1_ArmTemplate_1')]"
			]
		},
		{
			"name": "DFBK1_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blob_trigger_properties_pl_blob_trrigger_parameters_file_name": {
						"value": "[parameters('blob_trigger_properties_pl_blob_trrigger_parameters_file_name')]"
					},
					"blob_trigger_properties_typeProperties_scope": {
						"value": "[parameters('blob_trigger_properties_typeProperties_scope')]"
					},
					"tumbling_trigger_properties_pipeline_parameters_start_time": {
						"value": "[parameters('tumbling_trigger_properties_pipeline_parameters_start_time')]"
					},
					"tumbling_trigger_properties_pipeline_parameters_end_time": {
						"value": "[parameters('tumbling_trigger_properties_pipeline_parameters_end_time')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DFBK1_ArmTemplate_2')]"
			]
		}
	]
}